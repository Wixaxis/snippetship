<div class="row justify-content-between py-3 w-75 mx-auto">
  <h1 class="col h-100 p-0">Projects</h1>
  <button class="col-1 btn btn-success w-auto">New project</button>
</div>
<table class="table table-dark table-striped w-75 mx-auto">
  <thead>
    <tr>
      <th></th>
      <th scope="col">Name</th>
      <th scope="col">Folder path</th>
      <th scope="col">Users</th>
      <th></th>
    </tr>
  </thead>
  <tbody class="table-group-divider">
    <% Project.all.each do |p| %>
      <%= render partial: "project_element", locals: { project: p } %>
    <% end %>
  </tbody>
</table>
